"graph_offset": Vector2( 1020, 0 ),
"links": {
0: 7,
1: 4
},
"rect_size": Vector2( 324, 137 ),
"slot_amount": 2,
"text": "You will not disappoint me, right Scene?
<choice>Of course not!</choice>
