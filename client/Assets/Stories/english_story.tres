"type": "line"
},
19: {
"graph_offset": Vector2( 7140, 80 ),
"links": {
0: 15
},
"rect_size": Vector2( 324, 137 ),
"slot_amount": 1,
"text": "It 3024, making it around 1000 years since your death.",
